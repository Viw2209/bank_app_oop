// Create a base class for Saving Account
//attributes = account number, balance 
//abstract 

public abstract class SavingAccount {

    // add attributes
    // private = because this cannot be change

    private int accountNumber ;

    // checking account balance

    private double balance;

    // constructor 
    
    public SavingAccount() {

    }

    public SavingAccount(int accountNumber);
    this.accountNumber = accountNumber;
    balance = 0;

    }

    //Getter methods

    public double getBalance () {

    return this.balance
    }

    public int get.accountNumber(){
        return.this.accountNumber
    }

//Abstract
/**
 * Deposit funds into account if amount > 0
 * @param amount = money that will be deposit
 */

 public abstract void deposit(double amount);

 //Abstract
/**
 * withdraw funds from account
 * amount withdraw will be <= balance
 * @param amount = money to be withdrawn
 */

 public abstract void withdrawal(double amount);
}

